<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android">

<LinearLayout
    android:layout_marginTop="10sp"
    android:gravity="center"
    android:orientation="vertical"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
         >

        <LinearLayout
            android:background="#e1e1e1"

            android:id="@+id/l1"
            android:layout_margin="1dp"
            android:clickable="true"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical"
            android:layout_width="0dp"
            android:padding="20dp"

            android:layout_height="wrap_content">


            <TextView
                android:layout_gravity="center"

                android:layout_width="wrap_content"
                android:text="1"

                android:textSize="20sp"
                android:gravity="start"
                android:textStyle="bold"

                android:layout_height="wrap_content" />

        </LinearLayout>


        <LinearLayout
            android:layout_margin="1dp"
            android:padding="20dp"
            android:id="@+id/l2"

            android:layout_weight="1"
            android:gravity="center"
            android:background="#e1e1e1"
            android:orientation="vertical"
            android:layout_width="0dp"
            android:layout_height="wrap_content">


            <TextView
                android:layout_gravity="center"

                android:layout_width="wrap_content"
                android:text="2"

                android:textSize="20sp"
                android:gravity="start"
                android:textStyle="bold"

                android:layout_height="wrap_content" />

        </LinearLayout>

        <LinearLayout
            android:layout_margin="1dp"
            android:padding="20dp"
            android:id="@+id/l3"

            android:layout_weight="1"
            android:gravity="center"
            android:background="#e1e1e1"
            android:orientation="vertical"
            android:layout_width="0dp"
            android:layout_height="wrap_content">


            <TextView
                android:layout_gravity="center"

                android:layout_width="wrap_content"
                android:text="3"

                android:textSize="20sp"
                android:gravity="start"
                android:textStyle="bold"

                android:layout_height="wrap_content" />

        </LinearLayout>

    </LinearLayout>

    <LinearLayout

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        >

        <LinearLayout
            android:padding="20dp"
            android:background="#e1e1e1"
            android:layout_margin="1dp"
            android:clickable="true"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical"
            android:layout_width="0dp"
            android:id="@+id/l4"

            android:layout_height="wrap_content">


            <TextView
                android:layout_gravity="center"

                android:layout_width="wrap_content"
                android:text="4"

                android:textSize="20sp"
                android:gravity="start"
                android:textStyle="bold"

                android:layout_height="wrap_content" />

        </LinearLayout>

        <LinearLayout
            android:background="#e1e1e1"
            android:layout_margin="1dp"
            android:clickable="true"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical"
            android:layout_width="0dp"
            android:padding="20dp"
            android:id="@+id/l5"

            android:layout_height="wrap_content">


            <TextView
                android:layout_gravity="center"

                android:layout_width="wrap_content"
                android:text="5"

                android:textSize="20sp"
                android:gravity="start"
                android:textStyle="bold"

                android:layout_height="wrap_content" />

        </LinearLayout>
        <LinearLayout
            android:layout_margin="1dp"
            android:padding="20dp"
            android:id="@+id/l6"

            android:layout_weight="1"
            android:gravity="center"
            android:background="#e1e1e1"
            android:orientation="vertical"
            android:layout_width="0dp"
            android:layout_height="wrap_content">


            <TextView
                android:layout_gravity="center"

                android:layout_width="wrap_content"
                android:text="6"

                android:textSize="20sp"
                android:gravity="start"
                android:textStyle="bold"

                android:layout_height="wrap_content" />

        </LinearLayout>

    </LinearLayout>

    <LinearLayout

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        >

        <LinearLayout
            android:background="#e1e1e1"
            android:layout_margin="1dp"
            android:clickable="true"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical"
            android:layout_width="0dp"
            android:padding="20dp"
            android:id="@+id/l7"

            android:layout_height="wrap_content">


            <TextView
                android:layout_gravity="center"

                android:layout_width="wrap_content"
                android:text="7"

                android:textSize="20sp"
                android:gravity="start"
                android:textStyle="bold"

                android:layout_height="wrap_content" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/l8"

            android:layout_margin="1dp"
            android:clickable="true"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical"
            android:layout_width="0dp"
            android:padding="20dp"
            android:background="#e1e1e1"
            android:layout_height="wrap_content">


            <TextView
                android:layout_gravity="center"

                android:layout_width="wrap_content"
                android:text="8"

                android:textSize="20sp"
                android:gravity="start"
                android:textStyle="bold"

                android:layout_height="wrap_content" />

        </LinearLayout>
        <LinearLayout
            android:layout_margin="1dp"
            android:padding="20dp"
            android:id="@+id/l9"

            android:layout_weight="1"
            android:gravity="center"
            android:background="#e1e1e1"
            android:orientation="vertical"
            android:layout_width="0dp"
            android:layout_height="wrap_content">


            <TextView
                android:layout_gravity="center"

                android:layout_width="wrap_content"
                android:text="9"

                android:textSize="20sp"
                android:gravity="start"
                android:textStyle="bold"

                android:layout_height="wrap_content" />

        </LinearLayout>

    </LinearLayout>
    <LinearLayout

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        >

        <LinearLayout
            android:id="@+id/back"
            android:background="@android:color/holo_red_light"
            android:layout_margin="1dp"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical"
            android:layout_width="0dp"
            android:padding="20dp"

            android:layout_height="match_parent">




        </LinearLayout>


        <LinearLayout
            android:layout_margin="1dp"
            android:padding="20dp"
            android:id="@+id/l0"

            android:layout_weight="1"
            android:gravity="center"
            android:background="#e1e1e1"
            android:orientation="vertical"
            android:layout_width="0dp"
            android:layout_height="wrap_content">


            <TextView
                android:layout_gravity="center"

                android:layout_width="wrap_content"
                android:text="0"

                android:textSize="20sp"
                android:gravity="start"
                android:textStyle="bold"

                android:layout_height="wrap_content" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/done"
            android:background="@android:color/holo_green_light"
            android:layout_margin="1dp"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical"
            android:layout_width="0dp"
            android:padding="20dp"

            android:layout_height="match_parent">




        </LinearLayout>

    </LinearLayout>


</LinearLayout>
</merge>